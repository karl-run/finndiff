{
  "name": "finndiff-web",
  "version": "0.1.3",
  "private": true,
  "author": "Karl J. Over√•",
  "license": "MIT",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "classnames": "^2.2.5",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "material-ui": "^0.20.0",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-nano-spinner": "^1.2.0",
    "react-router-dom": "^4.2.2",
    "react-window-size-listener": "^1.0.10"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-js": "react-scripts build",
    "watch-js": "react-scripts start",
    "dev": "run-p watch-css watch-js",
    "start": "echo You should deploy from root, not web && exit 1",
    "build": "run-s build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "devDependencies": {
    "flow-bin": "^0.63.1",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.1.0",
    "source-map-explorer": "^1.5.0"
  },
  "proxy": "http://localhost:4000"
}
