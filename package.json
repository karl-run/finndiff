{
  "name": "finndiff",
  "version": "0.1.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lerna run --concurrency 1 --stream precommit",
    "postinstall": "lerna bootstrap",
    "tag": "lerna publish --skip-npm --yes --cd-version patch",
    "build": "cd packages/web && npm run build",
    "start": "cd packages/api && NODE_ENV=production npm run start",
    "dev-web": "cd packages/web && npm run dev",
    "dev-api": "cd packages/api && npm run dev",
    "dev": "run-p dev-api dev-web",
    "test-web": "cd packages/web && npm run build",
    "test-api": "cd packages/api && npm run test",
    "test": "run-p -l --aggregate-output test-web test-api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karl-run/finndiff.git"
  },
  "author": "Karl J. OverÃ¥",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karl-run/finndiff/issues"
  },
  "homepage": "https://github.com/karl-run/finndiff#readme",
  "lint-staged": {
    "packages/**src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "lerna": "^2.8.0",
    "npm-run-all": "^4.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2"
  }
}
